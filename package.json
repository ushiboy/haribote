{
  "name": "haribote",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "tsc && npm run clean && vite build",
    "build:mock": "npm run clean && NODE_ENV=development vite build",
    "clean": "rm -rf ./dist",
    "preview": "vite preview - --port 3000",
    "test": "TZ='Asia/Tokyo' jest",
    "msw:init": "msw init public/ --save",
    "gen:api": "npm run gen:api:auth && npm run gen:api:article",
    "gen:api:auth": "orval --input ./schema/auth.yml --output ./src/drivers/api/auth.ts",
    "gen:api:article": "orval --input ./schema/article.yml --output ./src/drivers/api/article.ts",
    "tsc": "tsc",
    "lint": "tsc && npm run lint:eslint",
    "lint:fix": "tsc && npm run lint:eslint:fix && npm run lint:prettier",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --write src"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.11",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.3",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.4.3",
    "msw": "^1.1.0",
    "orval": "^6.12.1",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
